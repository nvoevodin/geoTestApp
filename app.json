{
  "expo": {
    "name": "VolleyPal",
    "slug": "VolleyPal_Beta",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vcoolstudio.volleypal",
      "buildNumber": "1.0.3",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "something",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow VolleyPal always to use your location. Your location is used to check you in at the appropriate playground. Your location data are not used or stored in any other way or for any other purpose.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.vcoolstudio.volleypal",
      "versionCode": 4
    },
    "description": ""
  }
}